@model IEnumerable<MyAzureAD.Models.Item>

@{
    ViewBag.Title = "Completed";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Completed tasks</h3>
<p>
    @Html.ActionLink("Back to Tasks", "Index")
</p>
<p>
    Environment: <strong> @ViewData["environment"] </strong>
</p>  

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User)
        </th>
        <th>
            @Html.DisplayName("Completed Date")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompletedDate)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.Id })
           
        </td>
    </tr>
}

</table>
